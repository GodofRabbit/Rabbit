/* 通用样式 */
view{display:-webkit-box;display:-webkit-flex;display:-ms-flexbox;display:flex; -webkit-flex-wrap:wrap; -ms-flex-wrap:wrap; flex-wrap:wrap; font-size:28rpx; width:100%; -webkit-box-align: start; -webkit-align-items: flex-start; -ms-flex-align: start; align-items: flex-start;}
.grace-padding{padding:2%; width:96%;}
.grace-center{-webkit-box-pack:center;-webkit-justify-content:center;-ms-flex-pack:center;justify-content:center; text-align:center;}
.grace-h1{font-size:60rpx; line-height:1.8em;}
.grace-h2{font-size:50rpx; line-height:1.8em;}
.grace-h3{font-size:42rpx; line-height:1.8em;}
.grace-h4{font-size:36rpx; line-height:1.8em;}
.grace-h5{font-size:30rpx; line-height:1.8em;}
.grace-text{font-size:30rpx; line-height:2.2em;}
.grace-text image{width:100%; margin:20rpx 0;}
.grace-text-small{font-size:24rpx; line-height:1.8em;}
.grace-indent{text-indent:2em;}
.grace-blod{font-weight:700;}
.grace-common-bg{background:#F8F8F8;}
.grace-text-overflow{width:100%; overflow:hidden; -webkit-flex-wrap:nowrap; -ms-flex-wrap:nowrap; flex-wrap:nowrap; white-space:nowrap; -o-text-overflow:ellipsis; text-overflow:ellipsis;}
.grace-noborder{border:0 !important;}
.grace-nowrap{-webkit-flex-wrap:nowrap !important;-ms-flex-wrap:nowrap !important;flex-wrap:nowrap !important;}
.grace-space-between{-webkit-box-pack:justify !important;-webkit-justify-content:space-between !important;-ms-flex-pack:justify !important;justify-content:space-between !important;}
.grace-left-padding-wrap{width:96%; padding:12rpx 0; padding-left:4%; overflow:hidden; background:#FFFFFF;}
.grace-left-padding-wrap .grace-list{border-bottom:1rpx solid #D1D1D1; padding-left:0;}
.grace-left-padding-wrap .grace-list view text{color:#666; font-size:28rpx;}

/* 滚动区域 */
.grace-scroll-x{width:100%; display:-webkit-box; display:-webkit-flex; display:-ms-flexbox; display:flex; white-space:nowrap; height:auto; font-size:0;}
.grace-scroll-x .grace-items{width:355rpx; vertical-align:top; height:200rpx; margin:0 10rpx; display:-webkit-inline-box; display:-webkit-inline-flex; display:-ms-inline-flexbox; display:inline-flex;}
.grace-scroll-y{width:100%; display:-webkit-box; display:-webkit-flex; display:-ms-flexbox; display:flex; font-size:0; height:200rpx;}
.grace-scroll-y .grace-items{width:100%; height:200rpx;}

/* 标题及更多 */
.grace-title{width:100%; padding:10rpx 0;}
.grace-title .grace-text-small{color:#888;}
.grace-more-bottom{font-size:24rpx; -webkit-box-pack:center; -webkit-justify-content:center; -ms-flex-pack:center; justify-content:center; padding:15rpx 0; line-height:40rpx; color:#007AFF;}
.grace-more-bottom text{font-size:24rpx;}
.grace-more-r{display:block; width:auto; -webkit-flex-shrink:0; -ms-flex-negative:0; flex-shrink:0; font-size:22rpx;}
.grace-more-r text{font-size:22rpx;}
.grace-more-bottom .grace-border{display:block; padding:10rpx 28rpx; border:1px solid #007AFF; border-radius:10rpx; margin-top:8rpx;}

/* 动态背景 */
@-webkit-keyframes gradient{50%{background-position:100% 0;}}
@keyframes gradient{50%{background-position:100% 0;}}
.grace-gradient-bg{width:100%;
  background: -o-linear-gradient(45deg,#5a3694 0,#13bdce 33%,#0094d9 66%,#6fc7b5 100%);
  background: linear-gradient(45deg,#5a3694 0,#13bdce 33%,#0094d9 66%,#6fc7b5 100%);
  background-size: 400%; background-position:0 100%; -webkit-animation: gradient 7.5s ease-in-out infinite; animation: gradient 7.5s ease-in-out infinite; padding:50rpx 0;
}
.grace-gradient-bg view{color:#FFF;}

/* 栅格布局 */
.grace-rows{width:100%;}
.grace-rows .grace-items{width:33.3%;}
.grace-bg-green1{background:#63ba97;}
.grace-bg-green2{background:#79C48C;}
.grace-bg-green3{background:#49A761;}

/* 普通列表 */
.grace-list{display:-webkit-box;display:-webkit-flex;display:-ms-flexbox;display:flex; padding:20rpx; line-height:1.8em; overflow:hidden; width:710rpx; -webkit-flex-wrap:nowrap !important; -ms-flex-wrap:nowrap !important; flex-wrap:nowrap !important; -webkit-box-pack:justify; -webkit-justify-content:space-between; -ms-flex-pack:justify; justify-content:space-between;}
.grace-list view{width:auto; -webkit-flex-wrap:nowrap; -ms-flex-wrap:nowrap; flex-wrap:nowrap; -webkit-flex-shrink:0; -ms-flex-negative:0; flex-shrink:0;}
.grace-list view:last-child{-webkit-box-pack:right;-webkit-justify-content:right;-ms-flex-pack:right;justify-content:right;}
.grace-list view text{color:#666; font-size:22rpx;}
.grace-list-text{font-size:30rpx; line-height:2em; width:100%; -webkit-flex-shrink:1; -ms-flex-negative:1; flex-shrink:1;}
.grace-list-text text{font-size:26rpx; color:#666;}
.grace-list-imgs-r{width:56rpx; -webkit-flex-shrink:0; -ms-flex-negative:0; flex-shrink:0; margin-left:20rpx;}
.grace-list-imgs-l{width:56rpx; -webkit-flex-shrink:0; -ms-flex-negative:0; flex-shrink:0; margin-right:20rpx;}
.grace-list-imgs-arrow{width:40rpx; -webkit-flex-shrink:0; -ms-flex-negative:0; flex-shrink:0; -webkit-box-pack:center; -webkit-justify-content:center; -ms-flex-pack:center; justify-content:center; color:#888;}
.grace-list-mr{margin-right:20rpx;}

/* 多格图标 */
.grace-boxes{display:-webkit-box;display:-webkit-flex;display:-ms-flexbox;display:flex; width:18%; margin:0 1%; padding:10rpx 0; -webkit-flex-wrap:wrap !important; -ms-flex-wrap:wrap !important; flex-wrap:wrap !important;}
.grace-boxes-img{width:70%; margin:0 auto; padding-bottom:10rpx; font-size:0;}
.grace-boxes-img image{width:100%;}
.grace-boxes-text{line-height:2em; -webkit-box-pack:center; -webkit-justify-content:center; -ms-flex-pack:center; justify-content:center; font-size:22rpx;}

/* 图文列表 */
.grace-imgitems{display:-webkit-box;display:-webkit-flex;display:-ms-flexbox;display:flex; width:100%;}
.grace-imgitems .grace-items{width:48%; margin:10rpx 1%; overflow:hidden; font-size:0; position:relative;}
.grace-imgitems ._img{width:100%;}
.grace-imgitems-tips{width:auto; overflow:hidden; position:absolute; left:0; top:10rpx; color:#FFFFFF; height:36rpx;}
.grace-imgitems-tips text{display:block; padding-left:20rpx; overflow:hidden; background:rgb(255,67,67); font-size:20rpx; line-height:36rpx;}
.grace-imgitems-tips-border{width:0; height:0; font-size:0; line-height:0; overflow:hidden; border-right:19rpx solid transparent; border-left:19rpx solid #FF4343; border-top: 19rpx solid #FF4343; border-bottom:19rpx solid rgb(255,67,67);}
.grace-imgitems-tips-green text{background:#49A761 !important;}
.grace-imgitems-tips-green .grace-imgitems-tips-border{border-right:19rpx solid transparent; border-left:19rpx solid #49A761; border-top: 19rpx solid #49A761; border-bottom:19rpx solid #49A761;}
.grace-imgitems-text{margin:6rpx 1%; width:98%; font-size:26rpx; height:50rpx; line-height:50rpx;}
.grace-imgitems-more{-webkit-box-pack:justify;-webkit-justify-content:space-between;-ms-flex-pack:justify;justify-content:space-between; line-height:46rpx;}
.grace-imgitems-button{width:auto; border:1px solid #49A761; line-height:44rpx; padding:0 20rpx; color:#49A761; font-size:24rpx; border-radius:5rpx;}

/* 新闻列表 */
.grace-news-list{padding:12rpx 0;}
.grace-news-list navigator{display:block; width:100%; padding:12rpx 0; margin:12rpx 0;}
.grace-news-list-items{width:100%; -webkit-flex-wrap:nowrap; -ms-flex-wrap:nowrap; flex-wrap:nowrap; position:relative;}
.grace-news-list-img{width:200rpx; -webkit-flex-shrink:0; -ms-flex-negative:0; flex-shrink:0;}
.grace-news-list-title{width:100%; overflow:hidden;}
.grace-news-list-title-main{line-height:1.5em; font-size:32rpx; width:100%;}
.grace-news-list-title-desc{font-size:24rpx; display:block; color:#666; margin-top:12rpx; height:40rpx; line-height:40rpx;}
.grace-news-tips{width:auto; padding:0 12rpx; border-radius:5rpx; overflow:hidden; background:#FF4343; position:absolute; top:0; color:#FFFFFF; height:36rpx; line-height:36rpx; font-size:20rpx;}
.grace-news-tips-l{left:0;}
.grace-news-tips-r{right:0;}
.grace-news-list-info{width:100%; margin-top:10rpx; line-height:36rpx; font-size:24rpx; color:#666; -webkit-box-pack:justify; -webkit-justify-content:space-between; -ms-flex-pack:justify; justify-content:space-between;}
.grace-news-list-info view{font-size:24rpx; color:#666;}
.grace-news-list-info text{font-size:24rpx; color:#666; margin-right:10rpx;}
.grace-news-list-img-news{width:100%; -webkit-flex-wrap:wrap; -ms-flex-wrap:wrap; flex-wrap:wrap;}
.grace-news-list-imgs{-webkit-box-pack:justify;-webkit-justify-content:space-between;-ms-flex-pack:justify;justify-content:space-between; width:100%; padding:18rpx 0;}
.grace-news-list-imgs image{width:31.3%; margin:0 1%;}
.grace-news-list-img-big{width:100%; padding:18rpx 0;}
.grace-news-list-img-big image{width:100%;}

/* 选项卡 */
.grace-tab-title{white-space:nowrap; background:#FFFFFF;}
.grace-tab-title view{width:auto; padding:0 18rpx; margin-right:30rpx; line-height:80rpx; display: inline-block; text-align:center; border-bottom:4rpx solid #FFFFFF; font-size:32rpx;}
.grace-tab-current{border-bottom:4rpx solid #FF6354 !important; color:#FF6354;}
.grace-tab-swiper{width:100%; height:350rpx; padding:12rpx 0;}
.grace-tab-swiper swiper-item{width:100%;}
.grace-tab-swiper swiper-item navigator{line-height:70rpx; width:100%; display:block;}
.grace-tab-swiper-full{width:100%; height:auto;}
.grace-tab-swiper-full swiper-item{width:100%;}
.grace-tab-swiper-full scroll-view{width:100%; height:100%;}

/* 滚动公告 */
.grace-swiper-msg{width:100%; padding:12rpx 0; -webkit-flex-wrap:nowrap; -ms-flex-wrap:nowrap; flex-wrap:nowrap;}
.grace-swiper-msg-icon{width:50rpx; margin-right:20rpx;}
.grace-swiper-msg-icon image{width:100%; -webkit-flex-shrink:0; -ms-flex-negative:0; flex-shrink:0;}
.grace-swiper-msg swiper{width:100%; height:50rpx;}
.grace-swiper-msg swiper-item{line-height:50rpx;}

/* 星级 */
.grace-stars{padding:8rpx; -webkit-flex-wrap:nowrap; -ms-flex-wrap:nowrap; flex-wrap:nowrap; -webkit-box-pack:justify; -webkit-justify-content:space-between; -ms-flex-pack:justify; justify-content:space-between;}
.grace-stars-items{width:20%; margin:0 10rpx;}
.grace-stars-items image{width:100%;}

/* 折叠面板 */
.grace-accordion{background:#FFFFFF;}
.grace-accordion-items{overflow:hidden; border-bottom:1px solid #F2F3F4;}
.grace-accordion-items:last-child{border:none;}
.grace-accordion-title{width:96%; -webkit-box-pack:justify; -webkit-justify-content:space-between; -ms-flex-pack:justify; justify-content:space-between; padding:0 1% 0 3%; line-height:88rpx; height:88rpx; overflow:hidden; font-size:32rpx;}
.grace-accordion .grace-current{background:#F2F3F4;}
.grace-accordion-body{overflow:hidden;}

/* 底部加载更多 */
@-webkit-keyframes grace-rotate360{0%{-webkit-transform:rotate(0deg);transform:rotate(0deg);}
50%{-webkit-transform:rotate(180deg);transform:rotate(180deg);}
100%{-webkit-transform:rotate(360deg);transform:rotate(360deg);}}
@keyframes grace-rotate360{0%{-webkit-transform:rotate(0deg);transform:rotate(0deg);}
50%{-webkit-transform:rotate(180deg);transform:rotate(180deg);}
100%{-webkit-transform:rotate(360deg);transform:rotate(360deg);}}
.grace-loading{width:100%; -webkit-box-pack:center; -webkit-justify-content:center; -ms-flex-pack:center; justify-content:center; padding:16rpx 0; line-height:40rpx; color:#888;}
.grace-loading text{margin-left:12rpx;}
.grace-loading-icon{width:40rpx; height:40rpx; -webkit-box-pack:center; -webkit-justify-content:center; -ms-flex-pack:center; justify-content:center; line-height:40rpx; font-size:30rpx; text-align:center; font-family:"grace-iconfont" !important; -webkit-animation:grace-rotate360 1200ms infinite linear; animation:grace-rotate360 1200ms infinite linear;}
.grace-loading-icon:before {content:"\E9DB"; color:#888;}

/* 数字角标 */
.grace-badge{border-radius:38rpx; height:38rpx; line-height:38rpx; padding:0 13rpx; font-size:22rpx; background:#D1D1D1;}
.grace-badge-red{background:#DD524D; color:#FFF !important;}
.grace-badge-green{background:#4CD964; color:#FFF !important;}
.grace-badge-blue{background:#007AFF; color:#FFF !important;}
.grace-badge-yellow{background:#F0AD4E; color:#FFF !important;}

/* 遮罩层 */
.grace-mask{background:rgba(0, 0, 0, 0.6); position:fixed; width:100%; height:100%; left:0; top:0; z-index:1;}

/* 弹出层形式的广告 */
.grace-banner{width:70%; position:fixed; left:50%; top:50%; background:#FFF; border-radius:10rpx; z-index:99; -webkit-transform: translate(-50%, -50%); -ms-transform: translate(-50%, -50%); transform: translate(-50%, -50%);}

/* 文件添加及预览 */
.grace-add-file{padding:20rpx;}
.grace-add-btn{width:213rpx; margin:10rpx; background:#F5F5F5; padding:45rpx 0;}
.grace-add-btn view{font-size:26rpx; height:40rpx; line-height:40rpx; -webkit-box-pack:center; -webkit-justify-content:center; -ms-flex-pack:center; justify-content:center; color:#999999; width:100%;}
.grace-add-btn view:first-child{font-size:80rpx; height:80rpx; line-height:80rpx;}
.grace-add-file .garce-items{width:213rpx; height:213rpx; margin:10rpx; overflow:hidden; position:relative;}
.grace-add-file .garce-items image{width:100%;}
.grace-add-file .garce-items-media{width:213rpx; height:213rpx; margin:10rpx; overflow:hidden;}
.grace-add-file .garce-items-media ._video{width:213px; height:190rpx;}
.grace-add-file .grace-remove{height:40rpx; font-size:22rpx; -webkit-box-pack:center; -webkit-justify-content:center; -ms-flex-pack:center; justify-content:center; line-height:60rpx; overflow:hidden;}
.grace-add-file .garce-items .grace-close{font-family:"grace-iconfont" !important; width:46rpx; height:46rpx; position:absolute; z-index:1; right:10rpx; bottom:10rpx; font-size:46rpx; color:#FF0000; opacity:0.8}
.grace-add-file .garce-items .grace-close:before{content:"\E602";}

/* 表单 */
.grace-form{padding:0; width:100%; display:-webkit-box; display:-webkit-flex; display:-ms-flexbox; display:flex;}
.grace-form ._form{display:block; width:100%; overflow:hidden;}
.grace-form .grace-items{border-bottom:1px solid #F2F3F4; width:100%; -webkit-flex-wrap:nowrap; -ms-flex-wrap:nowrap; flex-wrap:nowrap; -webkit-box-pack:justify; -webkit-justify-content:space-between; -ms-flex-pack:justify; justify-content:space-between; padding:10rpx 0;}
.grace-form .grace-items .grace-label{width:130rpx; height:80rpx; line-height:80rpx; -webkit-flex-shrink:0; -ms-flex-negative:0; flex-shrink:0; overflow:hidden;}
.grace-form .grace-items ._input{width:98%; height:40rpx; line-height:40rpx; background:none; -webkit-flex-shrink:1; -ms-flex-negative:1; flex-shrink:1; border:0; text-align:right; padding:18rpx 2%; margin-left:40rpx;}
.grace-form-r{width:100%; padding:0 10rpx; display:block; overflow:hidden; -webkit-flex-shrink:1; -ms-flex-negative:1; flex-shrink:1; margin-left:40rpx;}
.grace-form picker{width:100%; height:80rpx; line-height:80rpx; background:none; border:0; text-align:right;}
.grace-form switch{margin-top:10rpx;}
.grace-form .grace-items picker text{-webkit-box-pack:right;-webkit-justify-content:right;-ms-flex-pack:right;justify-content:right; line-height:80rpx; font-size:28rpx;}
.grace-form .grace-items picker text:after{content:"\E601"; padding-left:10rpx; color:#888; font-family:"grace-iconfont" !important; font-size:30rpx;}
.grace-form ._textarea{width:98%; height:100rpx; line-height:2em; background:none; border:0; padding:8rpx 2%; font-size:28rpx;}
.grace-label-x{width:100%; padding:12rpx 0; -webkit-flex-shrink:1; -ms-flex-negative:1; flex-shrink:1; margin-left:40rpx;}
.grace-label-x ._label{margin:10rpx; font-size:28rpx;}
.grace-label-y{width:100%; padding:12rpx 0; -webkit-flex-shrink:1; -ms-flex-negative:1; flex-shrink:1; margin-left:40rpx;}
.grace-form radio-group, .grace-form checkbox-group{width:100%; display:-webkit-box; display:-webkit-flex; display:-ms-flexbox; display:flex; -webkit-flex-wrap:wrap; -ms-flex-wrap:wrap; flex-wrap:wrap;}
.grace-label-y ._label{margin:10rpx; font-size:28rpx; width:95%;}
.grace-items-wbg{background:#FFF; border:0 !important; border-radius:8rpx; padding:5rpx 0 !important;}
.grace-items-wbg ._input{text-align:left !important;}

/* 搜索 */
.grace-search{width:92%; padding:15rpx 4%; background:#FFFFFF; border-radius:66rpx; height:36rpx; overflow:hidden; -webkit-flex-wrap:nowrap; -ms-flex-wrap:nowrap; flex-wrap:nowrap;}
.grace-search-icon{width:36rpx; height:36rpx; line-height:36rpx; color:#007AFF; -webkit-box-pack:center; -webkit-justify-content:center; -ms-flex-pack:center; justify-content:center; font-family:"grace-iconfont" !important; font-size:30rpx; font-style:normal; -webkit-flex-shrink:0; -ms-flex-negative:0; flex-shrink:0;}
.grace-search-icon:before{content:"\E604"; color:#007AFF;}
.grace-search ._input{width:100%; background:#900; padding:0 20rpx; border:0; background:#FFF; height:28rpx; line-height:28rpx; 
margin:0; color:#000;}
.grace-search-clear:before{content:"\E78A" !important; color:#CCCCCC !important;}
.grace-search-remove:before{font-family:"grace-iconfont"; font-style:normal;content:"\E684" !important; color:#CCCCCC !important; font-size:40rpx; line-height:40rpx;}

/* 标签 */
.grace-tips{padding:10rpx 0;}
.grace-tips view{padding:0 20rpx; border-radius:30rpx; margin-right:15rpx; margin-bottom:15rpx; font-size:24rpx; line-height:46rpx; border:1px solid #D1D1D1; color:#666666; width:auto;}

/* 底部导航 */
.grace-footer{position:fixed; z-index:2; left:0; bottom:0; background:#FFFFFF; border-top:1px solid #F6F7F8; width:100%; height:90rpx; -webkit-flex-wrap:nowrap; -ms-flex-wrap:nowrap; flex-wrap:nowrap; overflow:hidden;}
.grace-footer .grace-input{width:100%; margin:15rpx 20rpx; padding:0 15rpx; background:#F4F5F6; height:60rpx; border-radius:60rpx; -webkit-flex-wrap:nowrap; -ms-flex-wrap:nowrap; flex-wrap:nowrap;}
.grace-input-icon{width:60rpx; height:60rpx; line-height:60rpx; font-size:30rpx; color:#000000; -webkit-box-pack:center; -webkit-justify-content:center; -ms-flex-pack:center; justify-content:center; -webkit-flex-shrink:0; -ms-flex-negative:0; flex-shrink:0;}
.grace-footer .grace-input ._input{width:100%; padding:10rpx 15rpx; height:40rpx; line-height:40rpx; font-size:26rpx; background:none; border:0;}
.grace-footer .icons{width:70rpx; height:60rpx; margin:15rpx 8rpx; -webkit-box-pack:center; -webkit-justify-content:center; -ms-flex-pack:center; justify-content:center; line-height:60rpx; -webkit-flex-shrink:0; -ms-flex-negative:0; flex-shrink:0; font-size:50rpx;}
